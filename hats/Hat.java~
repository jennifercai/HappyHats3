import javax.swing.*;
/**
 * This class creates a Hat that the chid can wear.
 * @author Cindy Lian
 * @author Jennifer Cai
 * @version 4.0, June 10, 2013
 */
public class Hat extends ImageIcon
{
  /**
   * Creates an ImageIcon with the specific hat.
   * @param loc - String to create ImageIcon
   */
  public Hat (String loc)
  {
    super (loc);
  }
}
