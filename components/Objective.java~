import javax.swing.*;
/**
 * This class creates an  Objective that the user can find. 
 * @author Cindy Lian
 * @author Jennifer Cai
 * @version 4.0, June 10, 2013
 */
public class Objective extends ImageIcon
{ 
  /**
   * Creates the Objective with the image.
   * @param loc String location of objective
   */
  public Objective (String loc)
  {
      super (loc);
  }
  
}